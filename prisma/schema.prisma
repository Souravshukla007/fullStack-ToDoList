generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
}

model Item {
  id        Int     @id @default(autoincrement())
  title     String  @db.VarChar(100)
  completed Boolean @default(false)
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt

  @@map("items")
}
